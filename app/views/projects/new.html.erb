</div>
<div class="projects text-center">
  <br>
  <div class="container">
  <h3><strong>New Project</strong></h3>
  <hr>
  <div class="row">
    <div class="col-xs-12">
  <%= simple_form_for @project do |f| %>
  <%= f.input :name %>
  <%= f.input :url %>
  <%= f.input :description %>
  <%= f.input :built_on %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.button :submit,class: "btn btn-primary" %>
  <% end %>
</div>
